procedure main {
  say "我可以告诉你美国的历届总统是谁"
  say "你想知道哪一届的总统？"
  listen timelimit 10 {
    has "第一届" ? A
    has "第二届" ? B
    has "第三届" ? C
    has "第四届" ? D
    has "第五届" ? E
    has "第六届" ? F
    has "第七届" ? G
    has "第八届" ? H
    has "第九届" ? I
