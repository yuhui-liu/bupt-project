    has "第十二届" ? L
    has "第十三届" ? M
    has "第十四届" ? N
    has "第十五届" ? O
    has "第十六届" ? P
    has "第十七届" ? Q
    has "第十八届" ? R
    has "第十九届" ? S
    has "第二十届" ? T
    has "第二十一届" ? U
    has "第二十二届" ? V
    has "第二十三届" ? W
    has "第二十四届" ? X
    has "第二十五届" ? Y
    has "第二十六届" ? Z
    has "第二十七届" ? AA
    has "第二十八届" ? AB
    has "第二十九届" ? AC
    has "第三十届" ? AD
    has "第三十一届" ? AE
    has "第三十二届" ? AF
    has "第三十三届" ? AG
    has "第三十四届" ? AH
    has "第三十五届" ? AI
    has "第三十六届" ? AJ
    has "第三十七届" ? AK
    has "第三十八届" ? AL
    has "第三十九届" ? AM
    has "第四十届" ? AN
    has "第四十一届" ? AO
    has "第四十二届" ? AP
    has "第四十三届" ? AQ
    has "第四十四届" ? AR
    has "第四十五届" ? AS
    has "第四十六届" ? AT
    has "第四十七届" ? AU
  }
}
procedure A { say "第一届总统是乔治·华盛顿" }
